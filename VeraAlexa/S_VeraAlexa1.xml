<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_URI</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Volume</name>
			<dataType>ui2</dataType>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>100</maximum>
				<step>1</step>
			</allowedValueRange>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Repeat</name>
			<dataType>ui2</dataType>
			<allowedValueRange>
				<minimum>1</minimum>
				<maximum>100</maximum>
				<step>1</step>
			</allowedValueRange>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_RoutineName</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Text</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Language</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_Device</name>
			<dataType>string</dataType>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>RunCommand</name>
			<argumentList>
				<argument>
					<name>Command</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RoutineName</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>RunRoutine</name>
			<argumentList>
				<argument>
					<name>RoutineName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RoutineName</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Device</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>Say</name>
			<argumentList>
				<argument>
					<name>Text</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Text</relatedStateVariable>
				</argument>
				<argument>
					<name>Language</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Language</relatedStateVariable>
				</argument>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Device</relatedStateVariable>
				</argument>
				<argument>
					<name>Volume</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Volume</relatedStateVariable>
				</argument>
				<argument>
					<name>Repeat</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Repeat</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>Up</name>
			<argumentList>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Device</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>Down</name>
			<argumentList>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Device</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>Mute</name>
			<argumentList>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Device</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetVolume</name>
			<argumentList>
				<argument>
					<name>GroupZones</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Device</relatedStateVariable>
				</argument>
				<argument>
					<name>DesiredVolume</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Volume</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>